                			<li>{{#link-to 'page' 'press'}}<img src="/static/assets/images/img/pages/home/press-dwdd.png" alt="De Wereld Draait Door" />{{/link-to}}</li>
                			<li>{{#link-to 'page' 'press'}}<img src="/static/assets/images/img/pages/home/press-volkskrant.png" alt="Volkskrant" />{{/link-to}}</li>
                		</ul>
                	</div>
                	
                    {{ view App.QuoteCarouselView }}
                    
                </div>
            </div>
        </section>
    </div>

{% endtplhandlebars %}
